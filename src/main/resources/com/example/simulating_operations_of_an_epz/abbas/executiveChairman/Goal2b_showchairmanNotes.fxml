<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="780.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.simulating_operations_of_an_epz.abbas.executiveChairman.Goal2bShowchairmanNotesController">
   <children>
      <TableView fx:id="noteTable" layoutX="2.0" layoutY="60.0" prefHeight="227.0" prefWidth="780.0">
         <columns>
            <TableColumn fx:id="titlecol" prefWidth="350.5" text="Title" />
            <TableColumn fx:id="categorycol" prefWidth="138.0" text="Category" />
            <TableColumn fx:id="datecol" prefWidth="121.0" text="Date" />
            <TableColumn fx:id="attachmentcol" prefWidth="169.0" text="Attachment" />
         </columns>
      </TableView>
      <Label alignment="CENTER" layoutY="-1.0" prefHeight="61.0" prefWidth="780.0" style="-fx-font-size: 18; -fx-font-weight: bold; -fx-background-color: yellow;" text="Showing All Notes" />
      <Button layoutX="105.0" layoutY="308.0" mnemonicParsing="false" onAction="#goBackOnActionButton" text="Go Back" />
      <Button layoutX="625.0" layoutY="308.0" mnemonicParsing="false" onAction="#deleteANoteOnActionbutton" text="Delete a note" />
   </children>
</AnchorPane>
